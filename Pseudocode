Sameed (031):
FUNCTION addStop(stopName)
    CREATE newNode
    SET newNode.name = stopName
    SET newNode.next = NULL

    IF routeHead IS NULL THEN
        SET routeHead = newNode
    ELSE
        SET temp = routeHead
        WHILE temp.next IS NOT NULL
            SET temp = temp.next
        END WHILE
        SET temp.next = newNode
    END IF

    INCREMENT historyTop
    SET routeHistory[historyTop] = CALL cloneRoute(routeHead)
    PRINT "Added stop: " + stopName
END FUNCTION
-------------------------------------------------------------------------
Saad (061)
FUNCTION addPassenger(name)
    SET passengerQueue[rearQ] = name
    INCREMENT rearQ
    PRINT "Passenger added to queue: " + name
END FUNCTION

FUNCTION undoLastRouteUpdate()
    IF historyTop >= 0 THEN
        DECREMENT historyTop
        IF historyTop >= 0 THEN
            SET routeHead = routeHistory[historyTop]
        ELSE
            SET routeHead = NULL
        END IF
        PRINT "Last route update undone."
    ELSE
        PRINT "No route updates to undo."
    END IF
END FUNCTION
